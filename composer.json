{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "knplabs/knp-paginator-bundle": "^5.1",
        "sensio/framework-extra-bundle": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "4.4.*",
        "symfony/twig-pack": "*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.4.*",
        "tetranz/select2entity-bundle": "^3.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "escapestudios/symfony2-coding-standard": "^3.8",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.8",
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/stopwatch": "*",
        "symfony/twig-bundle": "*",
        "symfony/web-profiler-bundle": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "fix": [
            "./vendor/bin/php-cs-fixer fix -v --rules='{\"no_superfluous_phpdoc_tags\":false}' ./src",
            "./vendor/bin/phpcbf --standard=/home/wwwroot/sf4/vendor/escapestudios/symfony2-coding-standard/Symfony ./src/"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "require": "4.4.*"
        }
    }
}
